{
  "name": "@web-component/markdown-renderer",
  "version": "0.0.2",
  "description": "A web-component to render markdown to html.",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "markdown-it": "^8.4.1",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.10.0",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colshacol/markdown-renderer.git"
  },
  "author": "Colton Colcleasure <colshacol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colshacol/markdown-renderer"
  },
  "homepage": "https://github.com/colshacol/markdown-renderer",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
